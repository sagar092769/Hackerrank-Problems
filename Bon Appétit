   static void bonAppetit(List<Integer> bill, int k, int b) {
     int val=0;
     for(int i=0;i<bill.size();i++){
       if(i==k){
           continue;
       }
       val=val+bill.get(i);
     }
     int pay=val/2;
     if(pay-b==0){
         System.out.println("Bon Appetit");
     }else{
         System.out.println(Math.abs(pay-b));
     }
    }
